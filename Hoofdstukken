public class Main {

    public static void main(String[] args) {
        Hoofdstuk matthew = new Hoofdstuk();
         System.out.println(getNaamHoofdstuk());


    }
    public static String getNaamHoofdstuk(){
        Hoofdstuk matthew = new Mattheüs();
        String naam =  matthew.getClass().getSimpleName();
        return naam;
    }
}
